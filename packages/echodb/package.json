{
  "name": "@dxos/echodb",
  "version": "1.0.0-beta.5",
  "description": "ECHO database.",
  "homepage": "https://github.com/dxos/echodb/tree/master/packages/echodb#readme",
  "bugs": {
    "url": "https://github.com/dxos/echodb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/echodb.git"
  },
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:protobuf && npm run build:babel",
    "build:babel": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\" --source-maps --copy-files",
    "build:protobuf": "pbjs -t json ./src/proto/data.proto -o src/proto/gen/data.json",
    "clean": "del-cli dist",
    "coverage": "jest --coverage --verbose src",
    "lint": "semistandard ./src/**/*.js",
    "test": "jest --rootDir ./src --verbose",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test && npm run build",
    "watch": "babel-watch ./src --out-dir ./dist --source-maps"
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@dxos/codec-protobuf": "^1.1.0-beta.0",
    "@dxos/crypto": "^0.0.1",
    "@dxos/data-client": "^1.0.0-beta.0",
    "debug": "^4.1.1",
    "hypercore-crypto": "^1.0.0",
    "lodash.merge": "^4.6.1",
    "protobufjs": "^6.8.8",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.5",
    "@types/jest": "^25.2.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-import": "^3.0.0",
    "del-cli": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.13.1",
    "jest": "^24.8.0",
    "semistandard": "^14.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
